<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.dao.CategoryInterface">


    <insert id="add">
        insert into jxc_category (id,name,remark) values (#{id},#{name},#{remark});
    </insert>

    <select id="selectAll" resultType="com.example.demo.entity.Category">
        select * from jxc_category;
    </select>

</mapper>